#include <iostream>

using namespace std;


int fibbonaci(int num1){
  int a=0, b=1, c=a+b;
  if(num1==1) return 0;
 
 if(num1==2 || num1==3)
return 1;
 
 if(num1>3){
   
 for(int i=3; i<num1; i++){
      a=b;
      b=c;
      c=a+b;
    }
	return c;
 }  

}


int main(){

 
 int num1;
 
 cout<<"Dame un numero "<<endl;
 
 cin>>num1;
 
 cout<<"La funcion de este numero es"<<fibbonaci(num1)<<endl;


  return 0;
  }
